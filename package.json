{
  "name": "serverless-aws-template",
  "version": "1.0.0",
  "description": "A template repo for how to structure serverless functions powered by aws lambda.",
  "keywords": [],
  "license": "ISC",
  "author": "Gareth Jones",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "rm -rf lib/* && tsc -p tsconfig.build.json",
    "deploy:production": "sls deploy --stage prod",
    "deploy:staging": "sls deploy --stage staging",
    "format": "prettier --write * --ignore-path .gitignore",
    "lint": "eslint '*/**/*.{js,ts,tsx}'",
    "test": "jest",
    "typecheck": "tsc -p . --noEmit"
  },
  "prettier": "prettier-config-ackama",
  "dependencies": {
    "@slack/webhook": "^6.0.0",
    "source-map-support": "^0.5.19"
  },
  "devDependencies": {
    "@jest/types": "^26.6.2",
    "@types/aws-lambda": "^8.10.70",
    "@types/eslint": "^7.2.6",
    "@types/jest": "^26.0.20",
    "@types/node": "^16.18.7",
    "@types/prettier": "^2.1.5",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "eslint": "^7.18.0",
    "eslint-config-ackama": "^2.0.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jest-formatting": "^2.0.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "prettier-config-ackama": "^0.1.2",
    "serverless": "^3.25.1",
    "serverless-plugin-scripts": "^1.0.2",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
