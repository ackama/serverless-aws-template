{
  "name": "serverless-aws-template",
  "version": "1.0.0",
  "description": "A template repo for how to structure serverless functions powered by aws lambda.",
  "keywords": [],
  "license": "ISC",
  "author": "Gareth Jones",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "rm -rf lib/* && tsc -p tsconfig.build.json",
    "deploy:production": "sls deploy --stage prod",
    "deploy:staging": "sls deploy --stage staging",
    "format": "prettier --write * --ignore-path .gitignore",
    "lint": "eslint '*/**/*.{js,ts,tsx}'",
    "test": "jest",
    "typecheck": "tsc -p . --noEmit"
  },
  "prettier": "prettier-config-ackama",
  "dependencies": {
    "@slack/webhook": "^7.0.2",
    "source-map-support": "^0.5.19"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.109",
    "@types/eslint": "^8.4.10",
    "@types/jest": "^29.2.4",
    "@types/node": "^16.18.7",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "eslint": "^8.29.0",
    "eslint-config-ackama": "^3.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^28.6.0",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.3.1",
    "prettier": "^3.3.2",
    "prettier-config-ackama": "^1.1.0",
    "serverless": "^3.38.0",
    "serverless-plugin-scripts": "^1.0.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.4.5"
  }
}
